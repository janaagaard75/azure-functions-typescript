{
  "azureFunctions.deploySubpath": ".",
  "azureFunctions.preDeployTask": "func: extensions install",
  "azureFunctions.projectLanguage": "JavaScript",
  "azureFunctions.projectRuntime": "~2",
  "azureFunctions.projectSubpath": "dist",
  "azureFunctions.templateFilter": "Verified",
  "cSpell.words": [
    "Bitbucket",
    "CONTENTAZUREFILECONNECTIONSTRING",
    "CONTENTSHARE",
    "INSTRUMENTATIONKEY",
    "MSDEPLOY",
    "aagaard",
    "appinsights",
    "circleci",
    "codeframe",
    "copyfiles",
    "dayjs",
    "daystochristmas",
    "functionapp",
    "junit",
    "lockfile",
    "memoized",
    "promisify",
    "readonly",
    "serverfarms",
    "serviceplan"
  ],
  "debug.internalConsoleOptions": "neverOpen",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.exclude": {
    "bin": true,
    "obj": true
  },
  "jest.debugCodeLens.showWhenTestStateIn": ["fail", "pass", "skip", "unknown"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  }
}
