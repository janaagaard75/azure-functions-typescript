{
  "name": "azure-functions-typescript",
  "version": "1.0.0",
  "description": "Azure Function written in TypeScript",
  "repository": "https://github.com/janaagaard75/azure-functions-typescript.git",
  "author": "Jan Aagaard <jan@aagaard.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn install --frozen-lockfile && webpack",
    "lint": "tslint src/**/*.ts",
    "prettify": "prettier --write src/**/*.ts",
    "start": "yarn run build && (cd dist; func host start)",
    "test": "jest",
    "test-ci": "mkdirp test-results && cross-env JEST_JUNIT_OUTPUT=test-results/unit-tests-results.xml jest --runInBand --ci --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@azure/functions": "1.0.1-beta2"
  },
  "devDependencies": {
    "@types/jest": "24.0.1",
    "@types/node": "10.*.*",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "jest": "24.1.0",
    "mkdirp": "0.5.1",
    "prettier": "1.16.4",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3"
  }
}
