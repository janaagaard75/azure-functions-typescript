{
  "name": "azure-functions-typescript",
  "version": "1.0.0",
  "description": "Azure Function written in TypeScript",
  "repository": "https://github.com/janaagaard75/azure-functions-typescript.git",
  "author": "Jan Aagaard <jan@aagaard.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && yarn install --frozen-lockfile && webpack",
    "lint": "tslint src/**/*.ts",
    "prettify": "prettier --write '{./,./src/**/}*.{js,json,ts}'",
    "start": "yarn run build && (cd dist; func host start)",
    "test": "jest",
    "test-ci": "rm -rf test-results && mkdir test-results && cross-env JEST_JUNIT_OUTPUT=test-results/unit-tests-results.xml jest --runInBand --ci --reporters=default --reporters=jest-junit"
  },
  "devDependencies": {
    "@azure/functions": "1.0.1-beta2",
    "@types/jest": "24.0.9",
    "@types/node": "10.*.*",
    "azure-functions-core-tools": "2.4.419",
    "copy-webpack-plugin": "5.0.0",
    "cross-env": "5.2.0",
    "jest": "24.1.0",
    "jest-junit": "6.3.0",
    "prettier": "1.16.4",
    "ts-jest": "24.0.0",
    "ts-loader": "5.3.3",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3333",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3"
  }
}
